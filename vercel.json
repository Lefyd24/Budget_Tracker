{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "MONGODB_CONNECTION_STRING": "@mongodb_connection_string"
    },
    "cleanUrls": true,
    "maxDuration": 30
  }
  